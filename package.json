  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
